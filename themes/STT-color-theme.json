		{
			"scope": "number",
			"settings": {
				"foreground": "#00FF44"
			}
		},
		{
			"scope": "operator",
			"settings": {
				"foreground": "#FFEE00"
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#00FF44"
			}
		},
		{
			"scope": "type",
			"settings": {
				"foreground": "#FF0044"
